<%= simple_form_for(@retention) do |f| %>
  <%= f.error_notification %>

  <div class="form-inputs">
    <%= f.input :retid %>
    <%= f.association :trainmodel %>
    <%= f.input :offer1 %>
    <%= f.input :offer2 %>
    <%= f.input :offer3 %>
    <%= f.input :range %>
    <%= f.input :modcheck %>
  </div>

  <div class="form-actions">
    <%= f.button :submit %>
  </div>
<% end %>
