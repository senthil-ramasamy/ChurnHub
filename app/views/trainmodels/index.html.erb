            <th></th>
          </tr>
        </thead>
        <tbody>
           <% @trainmodels.each do |trainmodel| %>
            <tr>
              <td><h4><%= trainmodel.modid %><h4></td>
              <td>
                <h3 class="head"><%= link_to trainmodel.modname , trainmodel  %></h3>
                <ul class="post-meta">
                  <li><i class="fa fa-pencil-square-o"></i><strong> Posted By:</strong> Senthil</li>
                  <li><i class="fa fa-clock-o"></i><strong> Posted On:</strong>12 - 07 - 2016</li>
                  <li><i class="glyphicon glyphicon-comment"></i><strong> Description Note</strong></li>
                </ul>
                <p><%= trainmodel.moddesc %></p>
                
              </td>
              <td><h4><% if(trainmodel.modcheck==true) %>Trained<% else %>Non-Trained<% end %><h4></td>
              <td><h4><%= link_to 'View !', trainmodel, class: "btn btn-primary btn-md" %></h4></td>
              
            </tr>
          </tr>
    <% end %>
  </tbody>
</table>
